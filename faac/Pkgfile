# Description: MPEG-4 audio codecs.
# URL: https://www.audiocoding.com/

name=faac
version=1.30
release=2
source=(https://github.com/knik0/faac/archive/${version/./_}/$name-${version/./_}.tar.gz)

build() {
	cd $name-${version/./_}

	./bootstrap
	./configure --prefix=/usr

	make
	make DESTDIR=$PKG install
}
