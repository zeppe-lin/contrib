# Description: OSINT framework for phone numbers
# URL:         https://github.com/sundowndev/PhoneInfoga

name=phoneinfoga-bin
version=2.3.1
release=1
source=(https://github.com/sundowndev/PhoneInfoga/releases/download/v$version/PhoneInfoga_Linux_x86_64.tar.gz)

build() {
	install -Dm755 ${name/-bin} -t $PKG/usr/bin/
}
